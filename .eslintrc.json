{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-expressions": "off",
    "guard-for-in": "off",
    "no-shadow": "off",
    "radix": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-restricted-syntax": "off",
    "import/newline-after-import": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true,
        "variables": true,
        "allowNamedExports": false
      }
    ]
  }
}
